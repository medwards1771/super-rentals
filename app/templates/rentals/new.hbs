<h1>Add a New Rental</h1>
<form class="form-horizontal">
  <p>
    <label for="title">Title</label>
    {{input type='text' id='title' value=model.title}}
  </p>
  <p>
    <label for="owner">Owner</label>  
    {{input type='text' id='owner' value=model.owner}}
  </p>
  <p>
    <label for="city">City</label>
    {{input type='text' id='city' value=model.city}}
  </p>
  <p>
    <label for="type">Type</label>
    {{input type='text' id='type' value=model.type}}
  </p>
  <p>
    <label for="image">Link to Image</label>
    {{input type='text' id='image' value=model.image}}
  </p>
  <p>
    <label for="type">Bedrooms</label>
    {{input type='text' id='bedrooms' value=model.bedrooms}}
  </p>
  <button id="submit" class="btn btn-primary" {{action 'save' model}}>Save</button>
</form>

{{outlet}}
